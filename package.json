{
  "name": "mkt-rank",
  "version": "1.0.0",
  "description": "Market ranking tracker for Naver Shopping and other platforms",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:queue": "tsx watch src/index-queue.ts",
    "api": "tsx src/start-api.ts",
    "build": "tsc && node scripts/copy-assets.js",
    "postbuild": "echo 'Build complete with assets!'",
    "start": "node dist/index.js",
    "start:queue": "node dist/index-queue.js",
    "test": "jest",
    "test:search": "tsx src/test-search.ts",
    "test:db": "tsx src/test-db.ts",
    "test:slot-keywords": "tsx src/test-slot-keywords.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "redis:start": "docker-compose up -d redis",
    "redis:stop": "docker-compose down"
  },
  "keywords": [
    "naver",
    "shopping",
    "ranking",
    "tracker",
    "market"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.19",
    "@types/cron": "^2.0.1",
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.4",
    "axios": "^1.6.5",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "pg": "^8.16.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
