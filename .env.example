# Supabase Configuration (대시보드용)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# Local PostgreSQL Configuration (메인 데이터 저장소)
LOCAL_PG_HOST=localhost
LOCAL_PG_PORT=5432
LOCAL_PG_DATABASE=mkt_rank_local
LOCAL_PG_USER=your_username
LOCAL_PG_PASSWORD=

# Naver API Configuration (여러 개 추가 가능)
# API 키는 쉼표로 구분하여 여러 개 입력 가능
# 형식: CLIENT_ID:CLIENT_SECRET,CLIENT_ID2:CLIENT_SECRET2
NAVER_API_KEYS=client_id1:client_secret1,client_id2:client_secret2

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info

# Scheduler Configuration
SCHEDULE_INTERVAL=0 * * * *  # Every hour

# Backup Configuration
BACKUP_PATH=/backup/shopping_rankings/