# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Naver API Keys (comma separated)
NAVER_API_KEYS=clientId1:secret1,clientId2:secret2

# Scheduler Configuration
SCHEDULE_INTERVAL=0 * * * *  # Every hour

# Queue Configuration
QUEUE_CONCURRENCY=3
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# API Server
API_PORT=3001

# Slot Keywords Configuration
ENABLE_SLOT_KEYWORDS=false  # true로 변경하면 활성화
SLOT_KEYWORD_FIELDS=product_name,search_term,keyword  # JSONB에서 추출할 필드명 (콤마 구분)

# 중첩 필드 예시:
# SLOT_KEYWORD_FIELDS=product.name,meta.search_keyword,tags.primary